      <Button fx:id="loadGameButton" layoutX="125.0" layoutY="270.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="250.0" text="Load Game" textAlignment="CENTER" />
      <Button fx:id="exitButton" layoutX="125.0" layoutY="320.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="250.0" text="Exit" textAlignment="CENTER" />
      <Label alignment="CENTER" layoutY="77.0" prefHeight="66.0" prefWidth="500.0" text="Neu Beginnings">
         <font>
            <Font size="42.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
